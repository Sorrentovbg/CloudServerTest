<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane fx:id="authWindow" prefHeight="154.0" prefWidth="304.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AuthDialogController">
   <children>
      <Pane prefHeight="200.0" prefWidth="328.0">
         <children>
            <Label layoutX="14.0" layoutY="30.0" prefHeight="25.0" prefWidth="56.0" text="Login">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="loginField" layoutX="104.0" layoutY="30.0" prefHeight="25.0" prefWidth="215.0" />
            <Label layoutX="14.0" layoutY="88.0" prefHeight="25.0" prefWidth="90.0" text="Password">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordField" layoutX="104.0" layoutY="88.0" prefHeight="25.0" prefWidth="215.0" />
            <Button fx:id="authOkButton" onAction="#sendAuthData" layoutX="141.0" layoutY="140.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="OK" />
            <Button fx:id="authCloseButton" onAction="#closeAuth" layoutX="234.0" layoutY="140.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="Cancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
